<% if logged_in? %>
  <script type="text/javascript">
    window.currentUser = <%= render(
      "api/users/user.json.jbuilder",
      user: current_user
    ).html_safe %>;
    if (window.currentUser.user) window.currentUser = window.currentUser.user;
  </script>
<% end %>

<main id="root"></main>
